<launch>
  <node pkg="pimouse_slam" name="lightsensors" type="lightsensors.py" required="true" />
  <node pkg="pimouse_slam" name="motors" type="motors.py" required="true" />
  <node pkg="pimouse_slam" name="wall_around" type="wall_around.py" required="true" />

  <node name="lidar_node" pkg="rplidar_ros" type="rplidarNode" required="true">
    <param name="serial_port" value="/dev/ttyUSB0"/>
    <param name="frame_id" value="base_link" />
  </node>
</launch>
